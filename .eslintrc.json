{
  "eslintConfig": {
    "extends": ["plugin:react/recommended", "react-app", "shared-config"],
    "rules": {
      "additional-rule": "warn",
      "no-unused-vars": ["error"]
    },
    "overrides": [
      {
        "files": ["**/*.ts?(x)"],
        "rules": {
          "additional-typescript-only-rule": "warn",
          "react/jsx-uses-react": 1
        }
      }
    ]
  }
}
